{
  "name": "games",
  "description": "for chromebooks",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/gauge-16.png",
      "32": "/images/gauge-38.png",
      "48": "/images/gauge-48.png",
      "128": "/images/gauge-128.png"
    }
  },
  "icons": {
    "16": "/images/gauge-16.png",
    "32": "/images/gauge-38.png",
    "48": "/images/gauge-48.png",
    "128": "/images/gauge-128.png"
  }
}
